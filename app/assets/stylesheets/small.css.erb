@media (max-width: 1200px) {
 #navbar-ex1-collapse {
    visibility: hidden;
}
/* Keep brand logo on the left*/
.navbar-header {
    float: none;
    display: block;
}

.navbar-toggle {
    position: relative;
    display: block;
}
.navbar-collapse {
    border-top: 1px solid transparent;
    box-shadow: inset 0 1px 0 rgba(255,255,255,0.1);
}
.navbar-fixed-top {
    top: 0;
    border-width: 0 0 1px;
}
.navbar-collapse.collapse {
    display: none!important;
}
.navbar-nav {
    float: right!important;
    text-align: right;
    margin-top: 7.5px;
}
.navbar-nav>li {
    float: none;
}
.navbar-nav>li>a {
    padding-top: 10px;
    padding-bottom: 10px;
}


.navbar-nav .dropdown-menu li {
  padding: 0;
}
.navbar-nav .dropdown-menu a {
  font-size: 12px;
  white-space: normal;
}
ul.nav.navbar-nav.navbar-right .dropdown-menu a:hover {
    color: black !important;
}
.navbar-nav .dropdown-menu ul {
  color:black;
  padding-left: 20px;
}
.multi-column{
  width:100% !important;
  height: 100% !important;
}
.navbar-nav .dropdown:hover > a {
  color: black !important;
  
}
.navbar-nav .inner-dropdown {
  display: none;
}
.navbar-nav .inner-dropdown.open {
  display: block;
}
.navbar-nav .inner-dropdown li {
  display: inline-block;
}
}